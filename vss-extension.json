{
  "manifestVersion": 1,
  "id": "azure-devops-mvn-mirror",
  "version": "1.0.1",
  "name": "Configure Maven Mirror",
  "publisher": "kharkevich",
  "public": true,
  "description": "Configure Maven Mirror in settings.xml step",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "screenshots": [
    {
      "path": "media/settings.png"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "azure",
    "deployment",
    "maven",
    "mvn"
  ],
  "scopes": [
    "vso.release"
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/kharkevich/azure-pipelines-maven-mirror"
    },
    "support": {
      "uri": "https://github.com/kharkevich/azure-pipelines-maven-mirror/issues"
    },
    "privacypolicy": {
      "uri": "https://kharkevich.org/ado-extensions/privacy-terms/"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/kharkevich/azure-pipelines-maven-mirror"
  },
  "contributions": [
    {
      "id": "mvn-mirror",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "mvn-mirror"
      }
    }
  ],
  "files": [
    {
      "path": "mvn-mirror/v0/dist"
    },
    {
      "path": "mvn-mirror/v0/node_modules"
    },
    {
      "path": "mvn-mirror/v0/icon.png"
    },
    {
      "path": "mvn-mirror/v0/task.json"
    },
    {
      "path": "mvn-mirror/v1/dist"
    },
    {
      "path": "mvn-mirror/v1/node_modules"
    },
    {
      "path": "mvn-mirror/v1/icon.png"
    },
    {
      "path": "mvn-mirror/v1/task.json"
    }
  ]
}